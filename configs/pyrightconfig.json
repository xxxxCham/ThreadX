{
    "include": [
        "src/threadx/data/providers",
        "src/threadx/indicators",
        "tests/test_token_diversity.py"
    ],
    "exclude": [
        "**/__pycache__",
        "**/.pytest_cache",
        "**/.mypy_cache",
        "**/node_modules",
        "**/.venv",
        "**/venv",
        "**/build",
        "**/dist",
        "**/*.egg-info",
        "apps/**",
        "benchmarks/**",
        "src/threadx/ui/**",
        "src/threadx/testing/**",
        "src/threadx/config/**",
        "src/threadx/utils/**",
        "src/threadx/backtest/**",
        "src/threadx/strategy/**",
        "src/threadx/optimization/**"
